create table profile_info(
idProfile INT not null AUTO_INCREMENT,
user VARCHAR(255) not null,
level INT not null,
gooAns INT not null,
answers INT not null,
picture VARCHAR(255) not null,
nextTip DATE not null,
primary key (idProfile)
);